{
  "version": 1,
  "projects": {
    "smarthome": {
      "root": "apps/smarthome",
      "sourceRoot": "apps/smarthome/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/smarthome",
            "index": "apps/smarthome/src/index.html",
            "main": "apps/smarthome/src/main.tsx",
            "polyfills": "apps/smarthome/src/polyfills.ts",
            "tsConfig": "apps/smarthome/tsconfig.app.json",
            "assets": [
              "apps/smarthome/src/favicon.ico",
              "apps/smarthome/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "./webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/smarthome/src/environments/environment.ts",
                  "with": "apps/smarthome/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "smarthome:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "smarthome:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/smarthome/.eslintrc",
            "tsConfig": [
              "apps/smarthome/tsconfig.app.json",
              "apps/smarthome/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/smarthome/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/smarthome/jest.config.js",
            "tsConfig": "apps/smarthome/tsconfig.spec.json"
          }
        }
      }
    },
